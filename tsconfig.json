{
  "compilerOptions": {
    "module": "esnext",
    "target": "es5",
    "lib": ["es6", "dom"],
    "sourceMap": true,
    "allowJs": true,
    "jsx": "react",
    "moduleResolution": "node",
    "rootDir": "src",
    "strict": true,
    "strictFunctionTypes": false,
    "baseUrl": ".",
    "paths": {
      "dashkit-ui": ["./src"]
    }
  },
  "exclude": [
    "node_modules"
  ]
}
